<% @interests.group_by(&:category).each do |category, interests| %>
	<strong><%= category %></strong> <br>
	
	<% interests.group_by(&:subcategory).each do |subcategory, interests| %>
	<strong><%= subcategory %></strong> <br>

		<% interests.each do |interest| %>
		<%= interest.name %> <br>
		<% end %>
	<% end %>
<% end %>
