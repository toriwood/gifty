<%= render 'show' %>

<div class="grid">
		<% user.following.each do |friend| %>
		<div class="well">
		<strong><h4><%= link_to User.find(friend).username, user_path(id: friend) %></h4></strong>
		<%= link_to image_tag(User.find(friend).image.url(:thumb), class: "profile-image"), user_path(id: friend) %>
		</div>
		<% end %>
</div>
